@{
}
<h1>Member List</h1>

<table class="table">
    <thead>
        <tr>
            <th>Member ID</th>
            <th>Email</th>
            <th>Company Name</th>
            <th>City</th>
            <th>Country</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var member in Model)
        {
            <tr>
                <td>@member.MemberId</td>
                <td>@member.Email</td>
                <td>@member.CompanyName</td>
                <td>@member.City</td>
                <td>@member.Country</td>
            </tr>
        }
    </tbody>
</table>

<a asp-controller="Home" asp-action="Index">Back to Home</a>